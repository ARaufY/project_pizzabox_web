@{
  ViewData["Title"] = "Customer Login";
}
<h1>@ViewData["Title"]</h1>
<form action="/customer" method="POST">

</form>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">
        Name
      </th>
    </tr>
  </thead>

  @foreach (var item in ViewBag.Customer)
  {
    <tr>
      <td>
        @item.Name
      </td>
    </tr>

  }
</table>